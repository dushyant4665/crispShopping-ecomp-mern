

{
  "version": 2,
  "builds": [
    {
      "src": "client/dbserver.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/subscribe",
      "dest": "client/dbserver.js"
    },
    {
      "src": "/(.*)",
      "dest": "client/build/$1"
    }
  ]
}
