{
  "version": 2,
  "builds": [
    {
      "src": "client/dbserver.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/build/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/client/dbserver.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/build/$1"
    }
  ],
  "env": {
    "MONGODB_URI": "MONGODB_URI",
    "OTHER_ENV_VARIABLES": "@other_env_variables"
  }
}
